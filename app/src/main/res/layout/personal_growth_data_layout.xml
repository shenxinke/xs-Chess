<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/personal_nsist_days_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_22"
        android:background="@mipmap/clock"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/personal_nsist_days"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_11"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        app:layout_constraintLeft_toRightOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toTopOf="@+id/personal_nsist_days_image" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="å¤©"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toRightOf="@+id/personal_nsist_days"
        app:layout_constraintTop_toTopOf="@+id/personal_nsist_days" />

    <ImageView
        android:id="@+id/personal_accuracyrate_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/greatgood"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_nsist_days_image" />

    <TextView
        android:id="@+id/personal_accuracyrate"
        android:layout_width="@dimen/dp_180"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_11"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        app:layout_constraintLeft_toRightOf="@+id/personal_accuracyrate_image"
        app:layout_constraintTop_toTopOf="@+id/personal_accuracyrate_image" />

    <ImageView
        android:id="@+id/personal_total_topic_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/timu"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_accuracyrate_image" />

    <TextView
        android:id="@+id/personal_total_topic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_11"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        app:layout_constraintLeft_toRightOf="@+id/personal_total_topic_image"
        app:layout_constraintTop_toTopOf="@+id/personal_total_topic_image" />


    <ImageView
        android:id="@+id/personal_playing_chess_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/vs"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_total_topic_image" />

    <TextView
        android:id="@+id/personal_playing_chess"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_30"
        android:layout_marginStart="@dimen/dp_11"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        app:layout_constraintLeft_toRightOf="@+id/personal_playing_chess_image"
        app:layout_constraintTop_toTopOf="@+id/personal_playing_chess_image" />


    <ImageView
        android:id="@+id/personal_friendes_chess_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/friends"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_playing_chess_image" />

    <TextView
        android:id="@+id/personal_friendes_chess"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_30"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="@dimen/dp_11"
        app:layout_constraintLeft_toRightOf="@+id/personal_friendes_chess_image"
        app:layout_constraintTop_toTopOf="@+id/personal_friendes_chess_image" />


    <ImageView
        android:id="@+id/personal_level_chess_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/level"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_friendes_chess_image" />

    <TextView
        android:id="@+id/personal_level_chess"
        android:layout_width="@dimen/dp_180"
        android:layout_height="wrap_content"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="@dimen/dp_11"
        app:layout_constraintLeft_toRightOf="@+id/personal_level_chess_image"
        app:layout_constraintTop_toTopOf="@+id/personal_level_chess_image" />

    <ImageView
        android:id="@+id/personal_transcend_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/chao"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_level_chess_image" />

    <TextView
        android:id="@+id/personal_transcend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="@dimen/dp_11"
        app:layout_constraintLeft_toRightOf="@+id/personal_transcend_image"
        app:layout_constraintTop_toTopOf="@+id/personal_transcend_image" />

    <ImageView
        android:id="@+id/personal_wrong_question_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/updatetimu"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_transcend_image" />

    <TextView
        android:id="@+id/personal_wrong_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="@dimen/dp_11"
        app:layout_constraintLeft_toRightOf="@+id/personal_wrong_question_image"
        app:layout_constraintTop_toTopOf="@+id/personal_wrong_question_image" />

    <ImageView
        android:id="@+id/personal_turnout_image"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:layout_marginTop="@dimen/dp_13"
        android:background="@mipmap/fanchao"
        app:layout_constraintLeft_toLeftOf="@+id/personal_nsist_days_image"
        app:layout_constraintTop_toBottomOf="@+id/personal_wrong_question_image" />

    <TextView
        android:id="@+id/personal_turnout"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_30"
        android:textColor="@color/color_d05701"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="@dimen/dp_11"
        app:layout_constraintLeft_toRightOf="@+id/personal_turnout_image"
        app:layout_constraintTop_toTopOf="@+id/personal_turnout_image" />

</android.support.constraint.ConstraintLayout>
