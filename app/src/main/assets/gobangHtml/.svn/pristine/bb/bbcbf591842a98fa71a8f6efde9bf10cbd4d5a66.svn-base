/* 光圈 */
.ripple{
	animation: ripple 1s  ease-in infinite
}
.ripple2{
	animation: ripple 1s 0.3s ease-in infinite
}
@keyframes ripple {
	0%,100%{
		opcity:0;
		transform: scale(0.3);
		background: rgba(106,227,255,0)
	}
	25%,50%{
		opcity:1;
		transform: scale(1);
		background: rgba(106,227,255,0.5)
	}
	
}
/* 小拇指 */
.finger{
	animation: finger 0.7s 0.5s infinite
}
@keyframes finger{
	0%,100%{
    	top: 30px;
    	right: -45px;
	}
	25%,50%{
		top: 60px;
    	right: -75px;
	}
}

/*答对了*/
.correct{
	animation: correct 0.25s  ease-in 
}
@keyframes correct {
	0%{
		opcity:0;
		transform: scale(0);
	}
	,100%{
		opcity:1;
		transform: scale(1);
	}
	
}