<script>
	$('#gameCanvas').attr('width',$('body').width()).attr('height',$('body').width());
	
	$(".tablist").find('li').click(function () {
		var $index = $(this).index();
		$(this).attr('class', 'active').siblings().removeAttr('class');
		$(".tab-content").find('.tab-pane').eq($index).show().siblings('.tab-pane').hide();
	});
	laydate.render({
		elem: "#jobStartTime",
		format: 'yyyy-MM-dd'
	});
	laydate.render({
		elem: "#jobEndTime",
		format: 'yyyy-MM-dd'
	});
	
    $("#smileImg").click(function(){
                         $("#emoji").toggle();
                         
                         })
	
	
</script>

