/*向左跳动*/
.toLeft{
	-webkit-animation: toLeft 0.6s infinite;
	        animation: toLeft 0.6s infinite;
}
@-webkit-keyframes toLeft{
	from{
		left: 0.1rem;
	}
	to{
		left: 0;
	}
}
@keyframes toLeft{
	from{
		left: 0.1rem;
	}
	to{
		left: 0;
	}
}
/*左-右晃动动画*/
.toRight{
	-webkit-animation: toRight 0.6s infinite;
	        animation: toRight 0.6s infinite;
}
@-webkit-keyframes toRight{
	from{
		right: 0.1rem;
	}
	to{
		right: 0rem;
	}
}
@keyframes toRight{
	from{
		right: 0.1rem;
	}
	to{
		right: 0rem;
	}
}

.toRight2{
	-webkit-animation: toRight2 0.3s 1;
	        animation: toRight2 0.3s 1;
}
@keyframes toRight2{
	from{
		right: -6.5rem;
	}
	to{
		right: 0rem;
	}
}
.toRight3{
	-webkit-animation: toRight3 0.3s 1;
	        animation: toRight3 0.3s 1;
}
@keyframes toRight3{
	from{
		right: 0rem;
	}
	to{
		right: -6.5rem;
		
	}
}
