#chessNews span{
    color: #722a00;
    font-size:0.14rem;
    padding: 0.04rem 0;
    display: block;
}
#power{
    width: 3.4rem;
    height: 3.2rem;
}
#game_control_tab_1{
    
    max-height:2.88rem;
    overflow-x:hidden;
    overflow-y:scroll;
}
#game_control_tab_2{
    height:3.6rem;
    max-height:3.6rem;
    overflow-x:hidden;
    overflow-y:scroll;
    
}
.multi{
    position:absolute;
    bottom:0;
    height: 5.98rem;
    z-index: 9999;
    background-color: #c7edfd;
    border-radius: 0.3rem;
    padding: 0.2rem 0.17rem;
    display:none;
}
.multi #triangle-down{
    display:block;
}
.multi #chat-wrapper{
    height:4.55rem !important;
    max-height: 4.4rem !important;
}
#triangle-down {
    width: 0;
    height: 0;
    border-left: 0.3rem solid transparent;
    border-right: 0.3rem solid transparent;
    border-top: 0.3rem solid #ff9419;
    position:absolute;
    top:-0.2rem;
    display:none;
}
#triangle-up {
    width: 0;
    height: 0;
    border-left: 0.3rem solid transparent;
    border-right: 0.3rem solid transparent;
    border-bottom: 0.3rem solid #ff9419;
    top:-0.1rem;
    left:50%;
    margin-left:-0.3rem;
}

.boxText{
    display:block;
    position: relative;
    line-height: 0.3rem;
    white-space: pre-wrap;
    word-wrap: break-word;
    word-break: break-all;
    border: solid 1px #f6a200;
    -moz-border-radius: 0.12rem;
    -webkit-border-radius: 0.12rem;
    border-radius: 0.12rem;
    padding:0.05rem;
    margin:0 0.1rem;
    align-items: center;
    font-size:0.14rem;
}
.boxText img{
    width:0.42rem;
    height:0.42rem;
}

/* 多人对弈小棋盘 */
#chess_boards_wrapper{
    padding:0.15rem 0.12rem;
}
#chess_boards_wrapper ul{
    list-style: none;
    display: flex;
    flex-flow: wrap;
    justify-content: space-around;
}
#chess_boards_wrapper ul li{
    font-size:0.15rem;
    margin: 0.1rem;
}


.oh{
    overflow: hidden;
}

.lh30{
    line-height:0.3rem
}





.z1{
    z-index: 1;
}




.shadow{
    border-radius: 0.66rem;
    box-shadow: 0.03rem 0px 0.92rem 0px rgba(100, 240, 253, 0.8);
}
.promotion{
    background: url(../../img/play/promotion.png) no-repeat center/100% 100%;
}
.friendship{
    background: url(../../img/play/friendship.png) no-repeat center/100% 100%;
}
.play_with_AI{
    background: url(../../img/play/play_with_AI.png) no-repeat center/100% 100%;
}
.scale13{
    transform: scale(1.16);
}

/* loading */
#optLoading{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.8);
    position: fixed;
    top:0;
    left:0;
    z-index: 1000;
}


#optLoading .loadingBox{
    width: 6.9rem;
    height: 2.3rem;
    text-align: center;
}
#optLoading .loadingBox p{
    font-size: 0.36rem;
    color:#fff;
    font-weight: bold;
    margin-bottom: 0.3rem;
    font-size: 0.2rem
}
.loadingBarbg{
    width:6.1rem;
    height: 0.54rem;
    background: url(../../img/loading/loadingBg.png) no-repeat center/100% 100%;
}
#optLoading button{
    width: 1.61rem;
    height: 0.72rem;
    background: url(../../img/loading/loadingQx.png) no-repeat center/100% 100%;
}
.loadingBar{
    width: 5.75rem;
    height: 0.26rem;
    position: absolute;
    top: 0.13rem;
    left: 0.19rem;
    border-radius: 0.15rem;
    overflow: hidden;
}

.loadingBar img{
    position: absolute;
    top:0.005rem;
    left:-100%
}
